{
	"name": "my-bot",
	"version": "1.0.0",
	"author": "Kevin Novak",
	"description": "A discord.js bot template written with TypeScript",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">=16.6.0"
	},
	"exports": [
		"./dist/start-bot.js",
		"./dist/start-manager.js"
	],
	"scripts": {
		"lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"clean": "git clean -xdf --exclude=\"/src/config/**/*\"",
		"clean:dry": "git clean -xdf --exclude=\"/src/config/**/*\" --dry-run",
		"build-types": "node scripts/compileTypes.js",
		"build": "npm run build-types && tsc --project tsconfig.json",
		"commands:view": "npm run build && node --enable-source-maps dist/start-bot.js commands view",
		"commands:register": "npm run build && node --enable-source-maps dist/start-bot.js commands register",
		"commands:rename": "npm run build && node --enable-source-maps dist/start-bot.js commands rename",
		"commands:delete": "npm run build && node --enable-source-maps dist/start-bot.js commands delete",
		"commands:clear": "npm run build && node --enable-source-maps dist/start-bot.js commands clear",
		"start": "npm run start:bot",
		"start:bot": "npm run build && node --enable-source-maps dist/start-bot.js",
		"start:manager": "npm run build && node --enable-source-maps dist/start-manager.js",
		"start:pm2": "npm run build && npm run pm2:start",
		"pm2:start": "pm2 start process.json",
		"pm2:stop": "pm2 stop process.json",
		"pm2:delete": "pm2 delete process.json",
		"api-knex": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex --knexfile=./knexfile.ts",
		"migrate": "npm run api-knex migrate:latest",
		"migrate:test": "NODE_ENV=TEST npm run api-knex migrate:latest",
		"test": "jest --no-cache --coverage src/"
	},
	"dependencies": {
		"@discordjs/rest": "1.1.0",
		"@faker-js/faker": "^7.5.0",
		"ajv-formats": "^2.1.1",
		"class-transformer": "0.5.1",
		"class-validator": "0.13.2",
		"date-fns": "^2.29.3",
		"discord-api-types": "0.37.5",
		"discord.js": "13.10.3",
		"discord.js-rate-limiter": "1.3.2",
		"express": "4.18.1",
		"express-promise-router": "4.1.1",
		"filesize": "9.0.11",
		"fishery": "^2.2.2",
		"json-schema": "^0.4.0",
		"knex": "^2.3.0",
		"linguini": "1.3.1",
		"luxon": "3.0.1",
		"node-fetch": "2.6.1",
		"node-schedule": "2.1.0",
		"objection": "^3.0.1",
		"pg": "^8.8.0",
		"pino": "8.4.0",
		"pino-pretty": "9.0.0",
		"pm2": "^5.2.0",
		"reflect-metadata": "^0.1.13",
		"remove-markdown": "0.5.0"
	},
	"devDependencies": {
		"@types/express": "4.17.13",
		"@types/jest": "^29.0.3",
		"@types/luxon": "3.0.0",
		"@types/node": "^18.7.1",
		"@types/node-schedule": "2.1.0",
		"@types/remove-markdown": "0.3.1",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"ajv": "^8.11.0",
		"eslint": "^8.21.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-unicorn": "^43.0.2",
		"glob": "^8.0.3",
		"jest": "^29.0.3",
		"json-schema-to-typescript": "^11.0.2",
		"prettier": "^2.7.1",
		"sqlite3": "^5.1.1",
		"ts-jest": "^29.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4"
	}
}
